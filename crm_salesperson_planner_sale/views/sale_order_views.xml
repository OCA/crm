<odoo>
    <record id="crm_salesperson_planner_sale_view_order_form" model="ir.ui.view">
        <field name="name">crm.salesperson.planner.sale.view.order.form</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='origin']" position="after">
                <field name="visit_id" />
            </xpath>
        </field>
    </record>
</odoo>
