<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_mailchimp_interest_form" model="ir.ui.view">
        <field name="model">mailchimp.interest</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="category_id" readonly="True" />
                    <field name="name" readonly="True" />
                </group>
            </form>
        </field>
    </record>
    <record id="view_mailchimp_interest_tree" model="ir.ui.view">
        <field name="model">mailchimp.interest</field>
        <field name="arch" type="xml">
            <tree>
                <field name="category_id" readonly="True" />
                <field name="name" readonly="True" />
            </tree>
        </field>
    </record>
</odoo>
