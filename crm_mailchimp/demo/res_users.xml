<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="user_subscriber" model="res.users" context="{'no_reset_password': True}">
        <field name="name">Mailchimp subscriber</field>
        <field name="login">mailchimp_subscriber</field>
        <field name="groups_id" eval="[(4, ref('group_subscriber'))]" />
    </record>
    <record id="base.user_demo" model="res.users" context="{'no_reset_password': True}">
        <field name="groups_id" eval="[(4, ref('group_user'))]" />
    </record>
</odoo>
